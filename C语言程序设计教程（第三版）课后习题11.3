#define _CRT_SECURE_NO_WARNINGS 1
#include<string.h>
#include<errno.h>
#include<stdlib.h>
#include<math.h>
#include<stdio.h>

typedef struct student
{
	int a;
	char b[5];
	int c1;
	int c2;
	int c3;
}S;
struct com
{
	S* data;
}C;
void input(struct com* a, int n)
{
	int i;
	for (i = 0; i < n; i++)
	{
		scanf("%d%s%d%d%d", &(a->data[i].a), a->data[i].b, &(a->data[i].c1), &(a->data[i].c2), &(a->data[i].c3));
	}
}
void print(struct com* a, int n)
{
	int i;
	for (i = 0; i < n; i++)
	{
		printf("%d,%s,%d,%d,%d\n", a->data[i].a, a->data[i].b, a->data[i].c1, a->data[i].c2, a->data[i].c3);
	}
}
int main()
{
	int n;
	scanf("%d", &n);
	C.data = (S*)malloc(n * sizeof(S));
	input(&C, n);
	print(&C, n);
	free(C.data);
	return 0;
}
